    "name": "Moritz Schwoerer",
    "email": "mr.mosch@gmail.com"
  },
  "contributors": [
    {
      "name": "Daniel Lehr",
      "email": "daniellehr@gmx.de"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/mosch/react-avatar-editor.git"
  },
  "license": "MIT",
  "keywords": [
    "react-component",
    "reactjs",
    "react",
    "canvas",
    "avatar",
    "editor",
    "profile",
    "image"
