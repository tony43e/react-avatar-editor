  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.10.4",
